<%= form_for @users, action: :create do |f| %>
    <%= f.label :uid, 'UID'  %>
    <%= f.text_field :uid %>
    <%= f.label :password, 'PASS'  %>
    <%= f.text_field :password %>
    <%= f.label :password_confirmation, 'PASS(確認)' %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit '登録' %>
<% end %>